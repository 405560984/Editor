{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch (source maps)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:1338/index-debug.html",
            "webRoot": "${workspaceRoot}/website/",
            "sourceMaps": true,
            "userDataDir": "${workspaceRoot}/.tempChromeProfileForDebug"
        },
        {
            "name": "Launch (without source maps)",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:1338/index-debug.html",
            "webRoot": "${workspaceRoot}/website/",
            "sourceMaps": false,
            "userDataDir": "${workspaceRoot}/.tempChromeProfileForDebug"
        },
        {
            "name": "Debug electron",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/electronPackages/BabylonJSEditor-darwin-x64/BabylonJSEditor.app/Contents/Resources/app/electron/main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}/electronPackages/BabylonJSEditor-darwin-x64/BabylonJSEditor.app/Contents/Resources/app/",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
            "runtimeArgs": [
                ".",
                "--enable-logging"
            ],
            "env": { },
            "sourceMaps": true,
            "outFiles": [
                
            ]
        }
    ]
}