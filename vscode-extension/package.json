{
    "name": "babylonjs-editor",
    "displayName": "Babylon.JS Editor",
    "description": "Babylon.JS Editor extension used to develop scripts using VSCode",
    "version": "0.0.1",
    "repository": "https://github.com/BabylonJS/Editor/",
    "publisher": "luaacro",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./.build/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "babylonjs-editor-plugin",
                    "title": "Babylon.JS Editor",
                    "icon": "assets/logo.png"
                }
            ]
        },
        "views": {
            "babylonjs-editor-plugin": [
                {
                    "id": "babylonjsEditorPlugin",
                    "name": "Babylon.JS Editor"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "compile": "tsc -p .",
        "watch": "tsc -watch -p .",
        "vscode:prepublish": "npm run compile",
        "package": "vsce package"
    },
    "dependencies": {
        "node-fetch": "2.3.0",
        "socket.io": "2.2.0",
        "uuid": "3.3.2",
        "babylonjs": "4.0.0-alpha.29",
        "babylonjs-gui": "4.0.0-alpha.29",
        "babylonjs-loaders": "4.0.0-alpha.29",
        "babylonjs-materials": "4.0.0-alpha.29",
        "babylonjs-procedural-textures": "4.0.0-alpha.29",
        "babylonjs-serializers": "4.0.0-alpha.29",
        "babylonjs-editor": "2.5.2",
        "ccapture.js": "~1.1.0",
        "es6-promise": "~4.1.0",
        "socket.io-client": "~2.0.4",
        "earcut": "~2.1.3",
        "javascript-astar": "~0.4.1",
        "litegraph.js": "~0.6.0",
        "spectorjs": "~0.9.2",
        "cannon": "~0.6.2"
    },
    "devDependencies": {
        "@types/node": "^8.10.25",
        "@types/socket.io": "2.1.2",
        "typescript": "~3.0.1",
        "vscode": "1.1.30",
        "vsce": "1.57.1"
    }
}
